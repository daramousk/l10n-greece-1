<odoo>

    <record id="tax_mechanism_printer_form" model="ir.ui.view">
        <field name="name">Tax Mechanism printer form</field>
        <field name="model">tax.mechanism.printer</field>
        <field name="arch" type="xml">
            <form cols="4">
                <label for="name" string="Name" />
                <field name="name" />
                <label for="ip" string="IP" />
                <field name="ip" />
                <label for="port" string="Port" />
                <field name="port" />
            </form>
        </field>
    </record>

</odoo>
